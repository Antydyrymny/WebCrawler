{
    "version": 2,
    "builds": [
        {
            "src": "assets/js/api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "assets/js/api/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
