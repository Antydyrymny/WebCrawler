{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/assets/js/$1"
        },
        {
            "src": "/server(.*)",
            "dest": "/assets/js/server$1"
        }
    ]
}
