{
    "version": 2,
    "functions": {
        "api/*": {
            "include": "root/assets/js/api/*",
            "memory": 512,
            "maxDuration": 10,
            "runtime": "vercel-node@0.12.0"
        }
    },
    "routes": [
        { "src": "/api/(.*)", "dest": "assets/js/api/server.js" },
        { "src": "/assets/js/(.*)", "dest": "assets/js/$1" },
        { "src": "/assets/css/(.*)", "dest": "assets/css/$1" },
        { "src": "(/.*)?", "dest": "/index.html" }
    ]
}
