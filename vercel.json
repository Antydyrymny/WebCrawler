{
    "version": 2,
    "builds": [
        {
            "src": "/assets/js/api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "assets/js/api/server.js" },
        { "src": "/assets/js/(.*)", "dest": "assets/js/$1" },
        { "src": "/assets/css/(.*)", "dest": "assets/css/$1" },
        { "src": "(/.*)?", "dest": "/index.html" }
    ]
}
